{
  "dependencies": {
    "express": "^4.17.1"
  },
  "name": "MultiplayerRacer",
  "version": "1.0.0",
  "scripts": {
    "build-server-dev": "webpack --mode development --config webpack.server.config.js",
    "build-client-dev": "webpack --mode development --config webpack.dev.config.js",
    "build-server-prod": "webpack --mode production --config webpack.server.config.js",
    "build-client-prod": "webpack --mode production --config webpack.prod.config.js",
    "start": "node ./dist/server.js",
    "build-dev": "npm-run-all --parallel build-client-dev build-server-dev",
    "build-prod": "npm-run-all --parallel build-client-prod build-server-prod"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.0.0",
    "html-webpack-plugin": "^4.5.2",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.0",
    "webpack": "^5.0",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-plugin": "^1.0.5"
  }
}
